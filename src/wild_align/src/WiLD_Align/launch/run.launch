<launch>

    <arg name="project" default="wild_align"/>
    
    <!-- Load Parameters-->
    <rosparam file="$(find wild_align)/config/params.yaml" command="load" />

    <!-- Launch CSLAM Nodes-->
    <include file="$(find wild_align)/launch/include/nodes.launch"/>

    <!-- Launch Rvis-->
    <include file="$(find wild_align)/launch/include/rviz.launch"/>
    
</launch>
