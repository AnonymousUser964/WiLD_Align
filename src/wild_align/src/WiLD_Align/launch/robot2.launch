<launch>
  <group ns="robot2">
    <arg name="project" default="wild_align"/>
    
    <!-- Load Parameters-->
    <rosparam file="$(find wild_align)/config/robot2.yaml" command="load" />

    <!-- Launch CSLAM Nodes-->
    <include file="$(find wild_align)/launch/include/nodes.launch"/>

    <!-- Launch Rvis-->
    <include file="$(find wild_align)/launch/include/rvizR2.launch"/>
    
  </group>
</launch>
